#include "lists.h"

/**
 * add_node - return number of elements
 *
 * @head: pointer to head of a list
 * @str: string length
 * Return: number of nodes
 *
 */


list_t *add_node(list_t **head, const char *str)
{
	unsigned int i = 0;

	while(str[i] != '\0')
		i++;
	*head->str = strdup(str);
	*head->len = i;
	*head->next = *head;
	*head = ptr;
	return(*head);
}
