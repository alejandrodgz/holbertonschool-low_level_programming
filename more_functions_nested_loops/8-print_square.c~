#include <stdio.h>
#include "main.h"

/**
 * print_square - print a line with n c length
 *
 * @size: length
 * Return: character
 */

void print_square(int size)
{
	int i, j;

	if (n <= 0)
	{
		_putchar ('\n');
	}
	for (i = 0; i < n; i++)
	{
		for (j = 0; j < n; j++)
		{
			_putchar (35);
		}
		_putchar ('\n');
		j = 0;
	}
}
