#include <stdio.h>
#include <stdlib.h>
#include <string.h>

/**
 * main - Write a program that prints its name
 *
 * @argc: number of parameters
 * @argv: strings
 * Return: name of program
 */

int main(int argc, char *argv[])
{
	if (argv[0] == 0)
		printf("0");
	printf("%d\n", argc);
	return (0);
}
