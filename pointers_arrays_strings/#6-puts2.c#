/**
 * puts2 - every other character
 * @str: pointer
 * Return: length of string
 */

void puts2(char *str)
{
	int len;

	len = strlen(*str);
	for (; *str <= len; str++)
	{
		if(*str / 2 != 0)
		{
		
	while (*str != '\0')
	{
		
		if
		_putchar(*str);
		str += 2;
	}
	_putchar('\n');
}
