#include "main.h"
#include <stdio.h>
#include <string.h>

/**
 * _memcpy - fills memory with a constant byte
 *
 * @dest: pointed area to fill
 * @src: what to fill
 * @n: number of bytes to fill
 *
 * Return: pointer to the memory area
 */

char *_memcpy(char *dest, char *src, unsigned int n)
{
	unsigned int i;

	for (i = 0; i < n; i++)
	{
		s[dest] = s[src];
	}
	return (dest);
}
