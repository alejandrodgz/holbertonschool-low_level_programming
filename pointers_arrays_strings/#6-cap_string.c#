#include "main.h"
#include <stdio.h>
#include <string.h>

/**
 * cap_string - from lowercase to uppercase
 * @s: number of bytes
 * Return: s upper
 */

char *cap_string(char *s)
{
	int i;

	for (i = 0; s[i] != '\0'; i++)
	{
		if(s[i] >= 97 && s[i] <= 122)
		{
			if
			s[i] = s[i] - 32;
		}
	}
	return (s);
}
